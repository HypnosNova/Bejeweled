precision mediump float;

//
// Translator library functions
//

float xlat_lib_saturate( float x) {
  return clamp( x, 0.0, 1.0);
}

vec2 xlat_lib_saturate( vec2 x) {
  return clamp( x, 0.0, 1.0);
}

vec3 xlat_lib_saturate( vec3 x) {
  return clamp( x, 0.0, 1.0);
}

vec4 xlat_lib_saturate( vec4 x) {
  return clamp( x, 0.0, 1.0);
}

mat2 xlat_lib_saturate(mat2 m) {
  return mat2( clamp(m[0], 0.0, 1.0), clamp(m[1], 0.0, 1.0));
}

mat3 xlat_lib_saturate(mat3 m) {
  return mat3( clamp(m[0], 0.0, 1.0), clamp(m[1], 0.0, 1.0), clamp(m[2], 0.0, 1.0));
}

mat4 xlat_lib_saturate(mat4 m) {
  return mat4( clamp(m[0], 0.0, 1.0), clamp(m[1], 0.0, 1.0), clamp(m[2], 0.0, 1.0), clamp(m[3], 0.0, 1.0));
}


//
// Structure definitions
//

struct VS_OUTPUT {
    vec4 Position;
    vec2 uv0;
    vec3 WorldNormal;
    vec3 WorldPosition;
};

struct PS_INPUT {
    vec2 uv0;
    vec3 WorldNormal;
    vec3 WorldPosition;
};


//
// Global variable definitions
//

uniform sampler2D Tex0;
uniform vec4 ambientLightColor;
uniform vec4 ambientMaterialColor;
uniform vec4 diffuseLightColor;
uniform vec4 diffuseMaterialColor;
uniform vec4 lightPosition;

//
// Function declarations
//

vec4 board_ps_main( in PS_INPUT xlat_var_input );

//
// Function definitions
//

vec4 board_ps_main( in PS_INPUT xlat_var_input ) {
    vec4 sampleColor;
    vec4 ambient;
    vec3 directionToLight;
    float diffuseLightIntensity;
    vec4 diffuse;
    vec4 color;

    sampleColor = texture2D( Tex0, xlat_var_input.uv0);
    ambient = (ambientMaterialColor * ambientLightColor);
    directionToLight = normalize( (vec3( lightPosition) - xlat_var_input.WorldPosition) );
    diffuseLightIntensity = xlat_lib_saturate( dot( directionToLight, xlat_var_input.WorldNormal) );
    diffuse = ((diffuseMaterialColor * diffuseLightColor) * diffuseLightIntensity);
    color = (sampleColor * (diffuse + ambient));
    color.w  = (sampleColor.w  * ambient.w );
    return color;
}


//
// User varying
//
varying vec4 xlat_varying_TEXCOORD0;
varying vec4 xlat_varying_TEXCOORD1;
varying vec4 xlat_varying_TEXCOORD2;

//
// Translator's entry point
//
void main() {
    vec4 xlat_retVal;
    PS_INPUT xlat_temp_xlat_var_input;
    xlat_temp_xlat_var_input.uv0 = vec2( xlat_varying_TEXCOORD0);
    xlat_temp_xlat_var_input.WorldNormal = vec3( xlat_varying_TEXCOORD1);
    xlat_temp_xlat_var_input.WorldPosition = vec3( xlat_varying_TEXCOORD2);

    xlat_retVal = board_ps_main( xlat_temp_xlat_var_input);

    gl_FragData[0] = vec4( xlat_retVal);
