precision mediump float;

//
// Structure definitions
//

struct VS_OUTPUT {
    vec4 Position;
    vec2 uv0;
    vec3 WorldNormal;
    vec3 WorldPosition;
};

struct PS_INPUT {
    vec2 uv0;
    vec3 WorldNormal;
    vec3 WorldPosition;
};


//
// Global variable definitions
//

uniform mat4 projection;
uniform mat4 view;
uniform mat4 world;

//
// Function declarations
//

VS_OUTPUT board_vs_main( in vec3 position, in vec3 normal, in float color, in vec2 uv0 );

//
// Function definitions
//

VS_OUTPUT board_vs_main( in vec3 position, in vec3 normal, in float color, in vec2 uv0 ) {
    mat4 WorldView;
    mat4 WorldViewProjection;
    VS_OUTPUT xlat_var_output;

    WorldView = ( view * world );
    WorldViewProjection = ( projection * WorldView );
    xlat_var_output.uv0 = uv0;
    xlat_var_output.Position = ( WorldViewProjection * vec4( position, 1.00000) );
    xlat_var_output.WorldPosition = vec3( ( world * vec4( position, 1.00000) ));
    xlat_var_output.WorldNormal = vec3( normalize( ( world * vec4( normal, 0.000000) ) ));
    return xlat_var_output;
}


//
// Attributes
//
attribute vec3 position;
attribute float color;
attribute vec2 texcoord0;

//
// User varying
//
varying vec4 xlat_varying_TEXCOORD0;
varying vec4 xlat_varying_TEXCOORD1;
varying vec4 xlat_varying_TEXCOORD2;

//
// Translator's entry point
//
void main() {
    VS_OUTPUT xlat_retVal;

    xlat_retVal = board_vs_main( vec3(position), vec3(gl_Normal), float(color), vec2(texcoord0));

    gl_Position = vec4( xlat_retVal.Position);
    xlat_varying_TEXCOORD0 = vec4( xlat_retVal.uv0, 0.0, 0.0);
    xlat_varying_TEXCOORD1 = vec4( xlat_retVal.WorldNormal, 0.0);
    xlat_varying_TEXCOORD2 = vec4( xlat_retVal.WorldPosition, 0.0);
}


